{
  "name": "@nordic-io/calixto",
  "version": "0.2.424",
  "description": "minimum. wonderful",
  "repository": {
    "type": "git",
    "url": "https://github.com/nordic-io/calixto.git"
  },
  "author": {
    "name": "VitorMendesco"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nordic-io/calixto/issues"
  },
  "homepage": "https://calixto.vercel.app",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "core/index.js",
  "module": "core/index.mjs",
  "types": "core/index.d.ts",
  "files": [
    "core"
  ],
  "eslintConfig": {
    "extends": [
      "./configs/.eslintrc"
    ]
  },
  "prettier": "./configs/.prettierrc.json",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "start": "npx serve storybook-static",
    "build": "vite build",
    "build:watch": "vite build -w",
    "build-storybook": "storybook build",
    "lint": "eslint src/**/*.{ts,tsx} && echo \"eslint: no lint errors\"",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx} && echo \"eslint: no lint errors\"",
    "lint:staged": "lint-staged -c ./configs/.lintstagedrc",
    "test": "vitest --c ./configs/vitest.config.ts run --silent",
    "test:coverage": "vitest --c ./configs/vitest.config.ts run --coverage --silent && open ./coverage/index.html",
    "test:related": "vitest related --c ./configs/vitest.config.ts --run --silent",
    "test:ui": "vitest --c ./configs/vitest.config.ts --ui --silent",
    "test:watch": "vitest --c ./configs/vitest.config.ts watch --silent",
    "upload": "yarn build && yarn publish",
    "plop": "plop --plopfile ./scripts/plop/plopfile.js",
    "husky": "husky install"
  },
  "peerDependencies": {
    "baseui": "^13.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "^6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@rollup/plugin-typescript": "^11.1.2",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-interactions": "^7.1.1",
    "@storybook/addon-links": "^7.1.1",
    "@storybook/blocks": "^7.1.1",
    "@storybook/react": "^7.1.1",
    "@storybook/react-vite": "^7.1.1",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "baseui": "^13.0.0",
    "chromatic": "^6.18.0",
    "eslint": "^8.46.0",
    "eslint-plugin-baseui": "^13.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "plop": "^3.1.2",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.1.1",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "^6.1.0",
    "terser": "^5.19.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.7",
    "vitest": "^0.33.0"
  }
}
