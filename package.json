{
  "name": "@nordic-io/calixto",
  "version": "0.2.424",
  "description": "minimum. wonderful",
  "repository": {
    "type": "git",
    "url": "https://github.com/nordic-io/calixto.git"
  },
  "author": {
    "name": "VitorMendesco"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nordic-io/calixto/issues"
  },
  "homepage": "https://calixto.vercel.app",
  "publishConfig": {
    "access": "public"
  },
  "main": "core/index.js",
  "module": "core/index.esm.js",
  "types": "core/index.d.ts",
  "files": [
    "core"
  ],
  "eslintConfig": {
    "extends": [
      "./configs/.eslintrc"
    ]
  },
  "prettier": "./configs/.prettierrc.json",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "start": "npx serve storybook-static",
    "build": "rollup -c ./configs/rollup.config.js --bundleConfigAsCjs",
    "build:watch": "rollup -c ./configs/rollup.config.js --bundleConfigAsCjs -w",
    "build-storybook": "storybook build",
    "husky": "husky install",
    "lint": "eslint src/**/*.{ts,tsx} && echo \"eslint: no lint errors\"",
    "lint:fix": "eslint --fix src/**/*.{ts,tsx} && echo \"eslint: no lint errors\"",
    "lint:staged": "lint-staged -c ./configs/.lintstagedrc",
    "test": "vitest --c ./configs/vitest.config.ts run --silent",
    "watch": "vitest --c ./configs/vitest.config.ts watch --silent",
    "coverage": "vitest --c ./configs/vitest.config.ts run --coverage --silent",
    "ui": "vitest --c ./configs/vitest.config.ts --ui --silent",
    "related": "vitest related --c ./configs/vitest.config.ts --run --silent",
    "upload": "yarn build && yarn publish"
  },
  "peerDependencies": {
    "baseui": "^12.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "^6.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/node": "^20.2.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.2.4",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.4",
    "@vitest/ui": "^0.31.4",
    "baseui": "^12.2.0",
    "chromatic": "^6.18.0",
    "eslint": "^8.42.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-baseui": "^12.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.12",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^3.20.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^7.0.18",
    "styletron-engine-atomic": "^1.5.0",
    "styletron-react": "^6.1.0",
    "tslib": "^2.5.3",
    "typescript": "5.0.4",
    "vite": "^4.3.9",
    "vitest": "^0.31.4"
  }
}
